# Environment Variables Template for GitLab MR Coach
# Copy this file to .env and fill in your actual values

# GitLab Configuration
GITLAB_TOKEN=your_gitlab_personal_access_token_here
CI_SERVER_URL=https://gitlab.com
CI_PROJECT_ID=your_project_id
CI_MERGE_REQUEST_IID=123

# Google AI Configuration
GOOGLE_AI_API_KEY=your_google_ai_api_key_here

# MongoDB Configuration (Optional - for metrics)
MONGODB_URI=mongodb+srv://username:password@cluster.mongodb.net/mr_coach

# Dashboard Configuration
PORT=3000

# Docker Hub (for building images)
DOCKER_HUB_USERNAME=your_dockerhub_username